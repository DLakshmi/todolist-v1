<%- include("header") -%>
  <div class="box " id="heading">
    <h1><%= ListTitle %></h1>
  </div>

  <div class="box">
    <% for (var i =0; i < newAddItems.length; i++) { %>

    <div class="item" >
      <input type="checkbox" >
      <p><%= newAddItems[i] %></p>
    </div>
    <%}%>

    <form class="" action="/" method="post">
      <input type="text" name="newItem" placeholder="Add New Item" autocomplete="off">
      <button type="submit" name="listButton" value= <%= ListTitle %> >+</button>
    </form>
  </div>

<%- include("footer") -%>
<!-- header and footer ejs =called layout  to avoid repetition-->
